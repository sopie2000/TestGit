<?xml version= "1.0" encoding ="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="kr.co.www.dao.main.MainDao">
    
    <select id="selectTestList" parameterType="CommonMap" resultType="CommonMap"  >
		SELECT * FROM TB_CUSTOMER_SUGGEST
    </select>
    
    
    <select id="selectTestCount" parameterType="CommonMap"  resultType="int" >
		SELECT 
			COUNT(*) AS TOTAL_COUNT
			FROM TB_CUSTOMER_SUGGEST
    </select>
    
</mapper>